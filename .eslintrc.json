{
  "extends": [
    "plugin:react/recommended",
    "naver",
    "plugin:prettier/recommended"
  ],
  "plugins": ["react", "prettier"],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "react/no-unknown-property": ["error", { "ignore": ["css"] }],
    "import/prefer-default-export": "off",
    "no-console": "off",
    "no-unused-vars": "off",
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  }
}
